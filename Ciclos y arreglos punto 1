def maxHR(age):
        return 208 - (0.7 * age)

def Zona_De_Trabajo(FCDE, maxHR2):
    percentage = (FCDE / maxHR2) * 100
    
    if 50 < percentage <= 60:
        return "Zona 1 (Recuperaci칩n)"
    elif 60 < percentage <= 70:
        return "Zona 2 (Resistencia)"
    elif 70 < percentage <= 80:
        return "Zona 3 (Aer칩bica)"
    elif 80 < percentage <= 90:
        return "Zona 4 (Anaer칩bica)"
    elif 90 < percentage <= 100: 
        return "Zona 5 (M치xima)"
    else: 
        return "Zona F (Zona fuera de rango)"

age = int(input("Ingrese su edad: "))
maxHR2 = maxHR (age)
Datos = []

n = int(input("Ingrese el nuemero de entrenamientos realizados: "))

for i in range (n):
    FCDE = int(input(f"Ingrese la frecuencia cardiaca durante el entrenamiento {i+1}: "))
    Datos.append (Zona_De_Trabajo (FCDE, maxHR2))

if FCDE > 50 or FCDE < 100:
    print (f"La zona de trabajo en la que se realizo el entrenamiento fue: {", ".join(Datos)}")
